<h1>Artists</h1>

<ul>
  <% @artists.each do |artist|%>
  <li>
    <%= artist.name %>
    <%= link_to 'Show', artist_path(artist) %>
    <%= link_to 'Edit', edit_artist_path(artist) %>
    <%= link_to 'Delete', artist_path(artist), method: :delete %>
  </li>
  <% end %>
<br/>
<br/>
</ul>

<%= link_to 'New Artist', new_artist_path %>
